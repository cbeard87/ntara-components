{
  "name": "@cbeard87/ntara-components",
  "version": "0.2.2",
  "description": "Stencil Component Starter",
  "author": {
    "name": "Christopher Beard",
    "email": "beard@ntara.com",
    "url": "https://github.com/cbeard87"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cbeard87/ntara-components.git"
  },
  "homepage": "https://github.com/cbeard87/ntara-components",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "generate": "stencil generate",
    "start": "stencil build --dev --watch --serve",
    "start-proxy": "npx concurrently -k \"npm:start\" \"ssh -R ntara:80:localhost:3333 serveo.net\"",
    "test": "stencil test --spec --e2e",
    "test-e2e": "stencil test --e2e",
    "test-spec": "stencil test --spec",
    "test-watch": "stencil test --spec --e2e --watchAll",
    "preversion": "npm run test-spec",
    "version": "npm run build && git add --all",
    "postversion": "git push && git push --tags"
  },
  "devDependencies": {
    "@stencil/core": "^1.6.1",
    "@stencil/postcss": "^1.0.1",
    "@stencil/sass": "^1.0.0",
    "@types/autoprefixer": "^9.6.1",
    "@types/jest": "^24.0.18",
    "@types/puppeteer": "1.20.1",
    "autoprefixer": "^9.6.4",
    "jest": "24.8.0",
    "jest-cli": "24.8.0",
    "puppeteer": "1.20.0"
  },
  "license": "BSD-3-Clause-Attribution",
  "dependencies": {
    "lodash.debounce": "^4.0.8",
    "stickybits": "^3.6.7"
  }
}
